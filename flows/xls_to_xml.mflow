<property name="message" value="Error on the DHL demo - xls-to-xml flow. Exception message: #[exception.getSummaryMessage()]"/>
<property name="priority" value="ERROR"/>
<property name="org.mule.tooling.ui.modules.core.widgets.meta.ModeAttribute" value="http://www.mulesoft.org/schema/mule/cloudhub/create-notification"/>
<property name="config-ref" value="Cloudhub"/>
<property name="auxiliary;index" value="28"/>
                            </properties>
                        </pattern>
                        <pattern name="Log Notification" type="http://www.mulesoft.org/schema/mule/core/logger" entity-id="Logger#4">
                            <graphicalData x="0" y="0" width="0" height="0"/>
