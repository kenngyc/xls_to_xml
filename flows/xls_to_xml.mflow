                    <property name="attachmentName" value="#[server.dateTime.format(&quot;ddMMyyy-hhmmss&quot;)]_#[message.inboundProperties.originalFilename].xml"/>
                    <property name="auxiliary;index" value="19"/>
                </properties>
            </pattern>
