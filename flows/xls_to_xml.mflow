    <global-element name="MS_Excel_To_XML" type="http://www.mulesoft.org/schema/mule/ee/data-mapper/config" entity-id="DataMapper#1">
        <properties>
            <property name="name" value="MS_Excel_To_XML"/>
            <property name="transformationGraphPath" value="ms_excel_to_xml.grf"/>
            <property name="auxiliary;index" value="3"/>
        </properties>
    </global-element>
    <global-element name="VM" type="http://www.mulesoft.org/schema/mule/vm/connector" entity-id="VM#1">
        <properties>
            <property name="dispatcherPoolFactory-ref"/>
            <property name="name" value="VM"/>
            <property name="validateConnections" value="true"/>
            <property name="queueTimeout"/>
            <property name="dynamicNotification"/>
            <property name="auxiliary;index" value="4"/>
            <property-collection name="@http://www.mulesoft.org/schema/mule/vm/queue-profile;0">
                <property name="isProcessed$$$$ " value="true"/>
                <property name="auxiliary;wrapIn" value="5"/>
                <property name="auxiliary;index" value="5"/>
                <property-collection name="@http://www.mulesoft.org/schema/mule/core/default-persistent-queue-store;1">
                    <property name="isProcessed$$$$ " value="true"/>
                    <property name="auxiliary;wrapIn" value="6"/>
                    <property name="auxiliary;index" value="6"/>
                </property-collection>
            </property-collection>
        </properties>
    </global-element>
    <global-element name="Cloudhub" type="http://www.mulesoft.org/schema/mule/cloudhub/config" entity-id="Cloudhub#1">
        <properties>
            <property name="username"/>
            <property name="name" value="Cloudhub"/>
            <property name="maxWaitTime"/>
            <property name="password"/>
            <property name="url"/>
            <property name="auxiliary;index" value="7"/>
        </properties>
    </global-element>
    <flow SubFlow="false" name="xls_to_xmlFlow" type="http://www.mulesoft.org/schema/mule/core/flow" entity-id="424394a1-0275-479c-ad8c-b9f3a7849e0b">
        <properties>
            <property name="initialState"/>
            <property name="processingStrategy2"/>
            <property name="processingStrategy"/>
            <property name="bussinessEventsLabel"/>
            <property name="tracking:enable-default-events" value="true"/>
            <property name="auxiliary;index" value="8"/>
        </properties>
        <lane hadOnlyOneElement="false" index="0" isChoiceOtherwiseLane="false" entity-id="compartment-lane#1">
            <graphicalData x="0" y="0" width="0" height="0"/>
